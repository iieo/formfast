{
  "name": "formfast",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write .",
    "types": "tsc --noEmit",
    "format:check": "prettier --check .",
    "lint": "next lint",
    "test": "vitest",
    "questions": "tsx ./src/filesystem/index.ts",
    "clean": "find . -type d \\( -name node_modules -o -name dist -o -name .next \\) -not -path \"./.vercel/*\" -prune -exec rm -rf {} \\;",
    "db:generate": "drizzle-kit generate --config=./src/db/drizzle.config.ts",
    "db:migrate": "dotenv -e .env tsx ./src/db/migrate.ts",
    "db:introspect": "dotenv -e .env -- drizzle-kit studio --config=src/db/drizzle.config.ts"
  },
