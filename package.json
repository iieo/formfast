{
  "name": "formfast",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "clean": "find . -type d \\( -name node_modules -o -name dist -o -name .next \\) -not -path \"./.vercel/*\" -prune -exec rm -rf {} \\;",
    "build": "dotenv -e .env next build",
    "start": "next start",
    "lint": "next lint",
    "types": "tsc",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:generate": "pnpm drizzle-kit generate:pg --config=src/db/drizzle.config.ts",
    "db:migrate": "dotenv -e .env -- pnpm tsx src/db/migrate.ts",
    "db:introspect": "dotenv -e .env -- drizzle-kit studio --config=src/db/drizzle.config.ts"
  },
  "engines": {
    "pnpm": "9"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "3.554.0",
    "@aws-sdk/client-sns": "3.583.0",
    "@aws-sdk/credential-provider-env": "3.535.0",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^7.0.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "3.3.4",
    "@radix-ui/react-checkbox": "1.0.4",
    "@radix-ui/react-collapsible": "1.0.3",
    "@radix-ui/react-dialog": "1.0.5",
    "@radix-ui/react-dropdown-menu": "2.0.6",
    "@radix-ui/react-icons": "1.3.0",
    "@radix-ui/react-label": "2.0.2",
    "@radix-ui/react-slot": "1.0.2",
    "@radix-ui/react-switch": "1.0.3",
    "@stripe/react-stripe-js": "2.7.1",
    "@stripe/stripe-js": "3.4.0",
    "@t3-oss/env-nextjs": "0.10.1",
    "@tanstack/react-table": "^8.17.3",
    "@types/mixpanel-browser": "2.49.0",
    "bcrypt": "5.1.1",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.0",
    "drizzle-orm": "0.30.8",
    "mixpanel": "^0.18.0",
    "mixpanel-browser": "2.50.0",
    "next": "14.2.1",
    "next-auth": "4.24.7",
    "postgres": "3.4.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "7.51.5",
    "react-hot-toast": "2.4.1",
    "react-markdown": "9.0.1",
    "sharp": "0.33.3",
    "stripe": "15.6.0",
    "tailwind-merge": "2.2.2",
    "tailwindcss-animate": "1.0.7",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@titanom/prettier-config": "2.0.0",
    "@types/bcrypt": "5.0.2",
    "@types/node": "20.12.7",
    "@types/react": "18.2.78",
    "@types/react-dom": "18.2.25",
    "dotenv-cli": "7.4.1",
    "drizzle-kit": "0.20.17",
    "eslint": "8.56.0",
    "eslint-config-next": "14.2.1",
    "pg": "8.11.5",
    "postcss": "8.4.38",
    "tailwindcss": "3.4.1",
    "tsx": "4.7.2",
    "typescript": "5.4.5"
  }
}
